    "version": "0.2.0",
    "configurations": [
        {
            "name": "CreateKerberosAuthenticationPolicy for Tier 0",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceRoot}\\CreateKerberosAuthenticationPolicy.ps1",
            "cwd": "${workspaceRoot}",
            "args": ["-PolicyName 'Test2' -Tier0ComputerGroup 'Tier 0 Computers'"]
        },
        {
            "name": "CreateKerberosAuthenticationPolicy for Tier 1",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceRoot}\\CreateKerberosAuthenticationPolicy.ps1",
            "cwd": "${workspaceRoot}",
            "args": ["-PolicyName 'Test2' -Tier0ComputerGroup 'Tier 0 Computers' -Tier1ComputerGroupName 'Tier 1 Computers' -Tier1KerberosAuthenticationPolicy"]
        },
        {
            "name": "PowerShell T0UserManagement.ps1 Tier0Computer 'OU=Tier 0,OU=Admin' -",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceRoot}\\Tier0UserManagement.ps1",
            "cwd": "${workspaceRoot}",
            "args": ["-PrivilegedOUPath 'OU=T0-GlobalAdmins,OU=Tier 0,OU=Admin' -KerberosPolicyName 'Tier 0 Restrictions'-PrivilegedServiceAccountOUPath 'OU=Tier 0 - Service Accounts,OU=Admin' -EnableMulitDomainSupport" -whthif]
        },
        {
            "name": "Tier0ComputerManagement default usage",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceRoot}\\Tier0ComputerManagement.ps1",
            "cwd": "${workspaceRoot}",
            "args": ["-Tier0ComputerGroupName 'Tier 0 Computers' -Tier0ComputerOU 'OU=Tier 0,OU=Admin'"]
        },
        {
            "name": "Tier0ComputerManagement no subdomains",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceRoot}\\Tier0ComputerManagement.ps1",
            "cwd": "${workspaceRoot}",
            "args": ["-Tier0ComputerGroupName 'Tier 0 Computers' -Tier0ComputerOU 'OU=Tier 0,OU=Admin' -MultiForest $False"]
        },
        {
            "name": "Tier0ComputerManagement multiple OUs",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceRoot}\\Tier0ComputerManagement.ps1",
            "cwd": "${workspaceRoot}",
            "args": ["-Tier0ComputerGroupName 'Tier 0 Computers' -Tier0ComputerOU 'OU=Tier,OU=Admin;OU=MyTier0' -MultiForest $False"]
        },
        {
            "name": "Tier1ComputerManagement with Tier0ComputerOU",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceRoot}\\Tier1MemberServerManagement.ps1",
            "cwd": "${workspaceRoot}",
            "args": ["-Tier1ComputerGroupName 'Tier 1 Servers' -Tier0ComputerOU 'OU=Tier 0,OU=Admin'"]
        },
        {
            "name": "Tier1ComputerManagement with Tier1ComputerOU",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceRoot}\\Tier1MemberServerManagement.ps1",
            "cwd": "${workspaceRoot}",
            "args": ["-Tier1ComputerGroupName 'Tier 1 Servers' -Tier1ComputerOU 'OU=BG-Servers'"]
        },
        {
            "name": "Tier1ComputerManagement with Tier0ComputerOU, any Operatingsystem andMulitDomain",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceRoot}\\Tier1MemberServerManagement.ps1",
            "cwd": "${workspaceRoot}",
            "args": ["-Tier1ComputerGroupName 'Tier 1 Servers' -Tier0ComputerOU 'OU=Tier 0,OU=Admin' -AnycomputerType -MulitDomainForest"]
        },
        {
            "name": "Launch Current File",
            "type": "PowerShell",
            "request": "launch",
            "script": "${file}",
            "args": []
        }
    ]
}